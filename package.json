{
  "name": "typeorm-playground",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/dougecomp/typeorm-playground.git",
  "license": "MIT",
  "scripts": {
    "prod": "yarn build && yarn start",
    "start": "node dist/index.js",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "dev": "tsnd --poll --exit-child --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "debug": "tsnd --inspect-brk --poll --exit-child --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.10",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "eslint": "^7.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.6.0",
    "sqlite3": "^5.0.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "class-validator": "^0.12.2",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "mssql": "^6.2.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.28",
    "uuid": "^8.3.1"
  }
}
